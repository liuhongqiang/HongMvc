<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mvc>
<!-- mvc config demo -->
<mvc>
	<global-results>
		<result name="error">/comm/error.jsp</result>
	</global-results>
	<package name="app">
		<action name="addApp" class="com.bonree.sdk.app.action.AppAction"
			method="add">
			<result name="success">/app/addApp.jsp</result>
		</action>
		<action name="getAppVersionJSON"
				class="com.bonree.sdk.app.action.AppVersionAction"
				method="getAppVersionJSON">
		</action>
	</package>
	
	<package name="config">
		<action name="reParamPage" 
				class="com.bonree.sdk.config.action.ParamAction"
				method="reParamPage">
			<result name="success">/config/param.jsp</result>
		</action>
	</package>
</mvc>
